class Solution:
    def merge(self, nums1: List[int], m: int, nums2: List[int], n: int) -> None:
        """
        Do not return anything, modify nums1 in-place instead.
        """

        numsSorted = nums1[:m] + nums2[:n]
        numsSorted.sort()
        while nums1:
            nums1.pop()
        for i in range(len(numsSorted)):
            nums1.append(numsSorted[i]) 
